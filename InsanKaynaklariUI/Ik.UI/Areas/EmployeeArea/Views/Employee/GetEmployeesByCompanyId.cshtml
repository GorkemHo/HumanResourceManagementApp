@{
}
@model List<EmployeeDto>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>User Name</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Second Last Name</th>
            <th>Birth Date</th>
            <th>Birth Place</th>
            <th>TC Identity</th>
            <th>Hire Date</th>
            <th>Termination Date</th>
            <th>Phone Number</th>
            <th>Address</th>
            <th>Salary</th>
            <th>Image Path</th>
            <th>Job Id</th>
            <th>Create Date</th>
            <th>Update Date</th>
            <th>Delete Date</th>
            <th>Status</th>
            <th>Department Id</th>
            <th>Company Id</th>
            <th>Role</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.UserName</td>
                <td>@employee.FirstName</td>
                <td>@employee.MiddleName</td>
                <td>@employee.LastName</td>
                <td>@employee.SecondLastName</td>
                <td>@employee.BirthDate</td>
                <td>@employee.BirthPlace</td>
                <td>@employee.TcIdentity</td>
                <td>@employee.HireDate</td>
                <td>@employee.TerminationDate</td>
                <td>@employee.PhoneNumber</td>
                <td>@employee.Address</td>
                <td>@employee.Salary</td>
                <td>@employee.ImageData</td>
                <td>@employee.JobId</td>
                <td>@employee.CreateDate</td>
                <td>@employee.UpdateDate</td>
                <td>@employee.DeleteDate</td>
                <td>@employee.Status</td>
                <td>@employee.DepartmentId</td>
                <td>@employee.CompanyId</td>
                <td>@employee.Role</td>
                <td>@employee.Email</td>
            </tr>
        }
    </tbody>
</table>
